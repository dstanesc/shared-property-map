{
  "name": "@dstanesc/shared-property-map",
  "description": "Super simplified property tree instantiation with data binding and distributed change notifications for Fluid testing and benchmarking",
  "version": "0.0.1",
  "homepage": "https://github.com/dstanesc/shared-property-map",
  "repository": "https://github.com/dstanesc/shared-property-map",
  "license": "MIT",
  "author": "Hexagon and contributors",
  "type": "module",
  "main": "./dist/workspace.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist node_modules",
    "build": "tsc",
    "test": "npm-run-all build test:jest",
    "dev": "tsc -w",
    "test:jest": "NODE_OPTIONS=--experimental-vm-modules cross-env FLUID_MODE=frs jest --runInBand dist"
  },
  "jest": {
    "testRegex": "(\\.(test|spec))\\.(js)$"
  },
  "devDependencies": {
    "@types/jest": "^28.1.5",
    "cross-env": "7.0.3",
    "jest": "^28.1.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@fluid-experimental/get-container": "1.2.0",
    "@fluid-experimental/property-binder": "1.2.0",
    "@fluid-experimental/property-changeset": "1.2.0",
    "@fluid-experimental/property-common": "1.2.0",
    "@fluid-experimental/property-dds": "1.2.0",
    "@fluid-experimental/property-inspector-table": "1.2.0",
    "@fluid-experimental/property-properties": "1.2.0",
    "@fluid-experimental/property-proxy": "1.2.0",
    "@fluid-experimental/schemas": "1.2.0",
    "@fluid-tools/benchmark": "^0.41.67945",
    "@fluidframework/azure-client": "1.0.1",
    "@fluidframework/data-object-base": "1.2.0",
    "@fluidframework/test-client-utils": "1.2.0",
    "@fluidframework/tinylicious-client": "1.2.0",
    "@microsoft/api-extractor": "^7.19.4",
    "uuid": "8.3.2"
  }
}
